@using Domain.Entities.Identity
@model List<ApplicationUser>


<div class="d-flex justify-content-between align-items-center">
    <h2>ApplicationUser</h2>
    <a asp-action="Create" class="btn btn-success">
        <i class="bi bi-plus"></i> Create New
    </a>
</div>

<table class="table table-striped">
    <thead>
        <tr>
<th>Id</th>
<th>AccessFailedCount</th>
<th>ConcurrencyStamp</th>
<th>Email</th>
<th>EmailConfirmed</th>
<th>FirstName</th>
<th>LastName</th>
<th>LockoutEnabled</th>
<th>LockoutEnd</th>
<th>NormalizedEmail</th>
<th>NormalizedUserName</th>
<th>PasswordHash</th>
<th>PhoneNumber</th>
<th>PhoneNumberConfirmed</th>
<th>SecurityStamp</th>
<th>TwoFactorEnabled</th>
<th>UserName</th>
<th style="width: 160px;">Action</th>
</tr>

    </thead>
    <tbody>
    @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
<td>@item.AccessFailedCount</td>
<td>@item.ConcurrencyStamp</td>
<td>@item.Email</td>
<td>@item.EmailConfirmed</td>
<td>@item.FirstName</td>
<td>@item.LastName</td>
<td>@item.LockoutEnabled</td>
<td>@item.LockoutEnd</td>
<td>@item.NormalizedEmail</td>
<td>@item.NormalizedUserName</td>
<td>@item.PasswordHash</td>
<td>@item.PhoneNumber</td>
<td>@item.PhoneNumberConfirmed</td>
<td>@item.SecurityStamp</td>
<td>@item.TwoFactorEnabled</td>
<td>@item.UserName</td>

                <td>
                    <div class="d-flex justify-content-between">
                        <a asp-area="lazycat" asp-controller="ApplicationUser" asp-action="Edit" asp-route-id="@item.Id" class="btn btn-primary">
                            <i class="bi bi-pencil"></i>
                        </a>
                        <a asp-area="lazycat" asp-controller="ApplicationUser" asp-action="Delete" asp-route-id="@item.Id" class="btn btn-danger ms-2">
                            <i class="bi bi-trash"></i>
                        </a>
                     </div>
                </td>
            </tr>
        }
    </tbody>
</table>